
<script type="text/javascript">

    $(document).ready(function  () {
        console.log("listo");
        var arr =
            {"options": null,
            "filters": null,
            "fdate": null,
            "tdate": null,
            "time": null };
        
        //var arr = [];
        //arr["options"] = null;
        //arr["filters"] = null;
        //arr["fdate"] = null; //fdate = from date
        //arr["tdate"] = null;//tdate = to date
        //arr["time"] = null;
        var x;

      

      
         $('.options > li').click(function() {
             x = $(this);
             
             if (arr['options'] === null) {

                 arr['options'] = x.attr('value');
             } else if (arr['options'] != null) {
                 arr['options'] = x.attr('value');
             }
            console.log(arr);


        });

        $('.filters > li').click(function() {
            x = $(this);
             
            if (arr['filters'] === null) {

                arr['filters'] = x.attr('value');
            } else if (arr['filters'] != null) {
                arr['filters'] = x.attr('value');
            }
            console.log(arr);


        });

        $('#fromDate').change(function () {
            x = $(this).val();
          
             
            if (arr['fdate'] === null) {

                arr['fdate'] = x;
                console.log(arr);
            } else if (arr['fdate'] != null) {
                arr['fdate'] = x;
            }
            console.log(arr);


        });

        $('#toDate').change(function () {
            x = $(this).val();


            if (arr['tdate'] === null) {

                arr['tdate'] = x;
                console.log(arr);
            } else if (arr['tdate'] != null) {
                arr['tdate'] = x;
            }
            console.log(arr);


        });
       
        $('#time').change(function () {
            x = $(this).val();
          
             
            if (arr['time'] === null) {

                arr['time'] = x;
                console.log(arr);
            } else if (arr['time'] != null) {
                arr['time'] = x;
            }
            console.log(arr);


        });

        $('#ready').click(function() {
            arr = JSON.stringify({ 'arr': arr });
            $.ajax({
                url: "/Visualizer/btnSubmit",
                type: "POST",
                data: arr,
                contentType: "json",
                datatype: 'aplication/json; chartset=utf-8',
                success: function(data) {
                    alert(data);
                }
            });
        });
        //if ($('#submit').click(function () {

        //    if (!arr["options"], !arr["filters"], !arr["fdate"], !arr["tdate"], !arr['time']) {
        //        alert('complete all the form');
        //    }

        //    $.each(arr, function(key, value) {
        //        $('.display-area').html($('.display-area').html() + '<br>' + key + '<span>' + value + '</br>');
        //    });


        //}));

    });


</script>
